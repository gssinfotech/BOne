<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   	
   	android:background="@drawable/app_background"
   >
   <LinearLayout 
       android:id="@+id/layout1"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:orientation="horizontal"
       >
        <FrameLayout 
        android:layout_width="wrap_content"
       android:layout_height="match_parent" 
       android:background="@android:color/white"
       android:layout_weight="2"
       android:layout_margin="10dp"
       >
           <FrameLayout 
        android:layout_width="match_parent"
       android:layout_height="match_parent" 
       android:background="@android:color/black"
       android:layout_margin="2dp"
       >
       <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
       android:text="Main Power"
       android:textSize="12sp"
       android:layout_margin="10dp"
       android:layout_gravity="center_horizontal"
       android:textColor="@android:color/white"
           />
       <TextView 
        android:layout_width="wrap_content"
       	android:layout_height="wrap_content" 
       	android:text="OFF"
       	android:textSize="15sp"
       	android:textStyle="bold"
        android:layout_marginTop="60dp"
       	android:layout_marginBottom="10dp"
       	 android:layout_gravity="center_horizontal"
       	 android:textColor="@color/health_OFF"
            />
          </FrameLayout>
       </FrameLayout>
         <FrameLayout 
        android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
       android:background="@android:color/white"
       android:layout_weight="1"
       android:layout_margin="10dp"
           >
           <FrameLayout 
        android:layout_width="match_parent"
       android:layout_height="wrap_content" 
       android:background="@android:color/black"
       android:layout_margin="2dp"
       >
        <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
        android:textSize="12sp"
       android:text="Battry Volts"
        android:layout_margin="10dp"
       android:textColor="@android:color/white"
        android:layout_gravity="center_horizontal"
           />
         <ImageView 
        android:layout_width="30dp"
       	android:layout_height="30dp" 
       	android:src="@drawable/battery123"
       	android:layout_marginTop="40dp"
       	android:layout_marginLeft="10dp"
       	android:layout_marginBottom="20dp"
       	android:layout_gravity="left"
            />
          <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
        android:textSize="12sp"
       android:text="4.46 Volts"
        android:layout_marginTop="60dp"
         android:layout_marginRight="10dp"
       	android:layout_marginBottom="20dp"
       android:textColor="@android:color/white"
        android:layout_gravity="right"
           />
          </FrameLayout>
       </FrameLayout>
   </LinearLayout> 
      
    <LinearLayout 
        android:id="@+id/layout2"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:layout_marginTop="20dp"
       android:orientation="horizontal"
       android:layout_below="@id/layout1"
       >
           <FrameLayout 
        android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
       android:background="@android:color/white"
       android:layout_weight="1"
       android:layout_margin="10dp"
           >
           <FrameLayout 
        android:layout_width="match_parent"
       android:layout_height="wrap_content" 
       android:background="@android:color/black"
       android:layout_margin="2dp"
       >
       <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
        android:textSize="12sp"
       android:text="Temperature"
       android:layout_margin="10dp"
       android:textColor="@android:color/white"
        android:layout_gravity="center_horizontal"
           />
        <ImageView 
        android:layout_width="30dp"
       	android:layout_height="30dp" 
       	android:src="@drawable/thermometer43"
       	android:layout_marginTop="60dp"
       	android:layout_marginLeft="10dp"
       	android:layout_marginBottom="20dp"
       	android:layout_gravity="left"
            />
           <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
        android:textSize="12sp"
       android:text="25'C"
        android:layout_marginTop="75dp"
         android:layout_marginRight="10dp"
       	android:layout_marginBottom="20dp"
       android:textColor="@android:color/white"
        android:layout_gravity="right"
           />
          </FrameLayout>
       </FrameLayout>
       
           <FrameLayout 
        android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
       android:background="@android:color/white"
       android:layout_weight="1"
       android:layout_margin="10dp"
           >
           <FrameLayout 
        android:layout_width="match_parent"
       android:layout_height="wrap_content" 
       android:background="@android:color/black"
       android:layout_margin="2dp"
       >
        <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
        android:textSize="12sp" 
        android:text="LIght Intensity"
        android:layout_margin="10dp"
        android:textColor="@android:color/white"
         android:layout_gravity="center_horizontal"
           />
          <ImageView 
        android:layout_width="30dp"
       	android:layout_height="30dp" 
       	android:src="@drawable/light91"
       	android:layout_marginTop="60dp"
       	android:layout_marginLeft="10dp"
       	android:layout_marginBottom="20dp"
       	android:layout_gravity="left"
            />
          <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
        android:textSize="12sp"
       android:text="200 Lux"
        android:layout_marginTop="75dp"
         android:layout_marginRight="10dp"
       	android:layout_marginBottom="20dp"
       android:textColor="@android:color/white"
        android:layout_gravity="right"
           />
          </FrameLayout>
       </FrameLayout>
        
           <FrameLayout 
        android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
       android:background="@android:color/white"
       android:layout_weight="1"
       android:layout_margin="10dp"
           >
           <FrameLayout 
        android:layout_width="match_parent"
       android:layout_height="wrap_content" 
       android:background="@android:color/black"
       android:layout_margin="2dp"
       >
         <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
        android:textSize="12sp"
       android:text="Qi Charging"
       android:layout_margin="10dp"
       android:layout_gravity="center_horizontal"
      android:textColor="@android:color/white"
           />
         <ImageView 
        android:layout_width="30dp"
       	android:layout_height="30dp" 
       	android:src="@drawable/charging"
       	android:layout_marginTop="60dp"
       	android:layout_marginLeft="10dp"
       	android:layout_marginBottom="20dp"
       	android:layout_gravity="left"
            />
         <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
        android:textSize="12sp"
       android:text="OFF"
        android:layout_marginTop="75dp"
         android:layout_marginRight="10dp"
       	android:layout_marginBottom="20dp"
       android:textColor="@color/health_OFF"
        android:layout_gravity="right"
           />
          </FrameLayout>
       </FrameLayout>
   </LinearLayout> 

    
    
    <LinearLayout 
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:orientation="horizontal"
       android:layout_marginTop="20dp"
       android:layout_below="@id/layout2"
       >
         <FrameLayout 
        android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
       android:background="@android:color/white"
       android:layout_weight="1"
       android:layout_margin="10dp"
           >
           <FrameLayout 
        android:layout_width="match_parent"
       android:layout_height="wrap_content" 
       android:background="@android:color/black"
       android:layout_margin="2dp"
       >
       <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
       android:text="Qi Current"
       android:layout_margin="10dp"
        android:textSize="12sp"
       android:layout_gravity="center_horizontal"
       android:textColor="@android:color/white"
           />
       <ImageView 
        android:layout_width="30dp"
       	android:layout_height="30dp" 
       	android:src="@drawable/current"
       	android:layout_marginTop="40dp"
       	android:layout_marginLeft="10dp"
       	android:layout_marginBottom="20dp"
       	android:layout_gravity="left"
            />
        <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
        android:textSize="12dp"
       android:text="3100 mAh"
        android:layout_marginTop="50dp"
         android:layout_marginRight="10dp"
       	android:layout_marginBottom="20dp"
       android:textColor="@android:color/white"
        android:layout_gravity="right"
           />
        </FrameLayout>
       </FrameLayout>
       <FrameLayout 
        android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
       android:background="@android:color/white"
       android:layout_weight="2"
       android:layout_margin="10dp"
           >
           <FrameLayout 
        android:layout_width="match_parent"
       android:layout_height="wrap_content" 
       android:background="@android:color/black"
       android:layout_margin="2dp"
       >
        <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content" 
       android:layout_weight="1"
       android:text="Qi Control"
        android:textSize="12sp"
       android:layout_gravity="center_horizontal"
       android:layout_margin="2dp"
       android:textColor="@android:color/white" />
       
        <ImageView 
        android:layout_width="30dp"
       	android:layout_height="30dp" 
       	android:src="@drawable/power_on"
       	android:layout_marginTop="40dp"
       	android:layout_marginBottom="20dp"
       	android:layout_gravity="center_horizontal"
            />
        </FrameLayout>
       </FrameLayout>
     
   </LinearLayout> 
    
    

</RelativeLayout>
