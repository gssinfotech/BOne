<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#25242a" 
    android:orientation="vertical">
    
    
    <LinearLayout 
        android:id="@+id/health_main_layout"
      	android:layout_width="match_parent"
    	android:layout_height="wrap_content"
    	android:background="#25242a" 
    	android:orientation="horizontal"
    	android:layout_marginTop="40dp"
    	  	
        >
        
        <Button 
        android:id="@+id/health_main_power"
      	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_weight="1"
    	android:text="" 
    	android:gravity="center_horizontal|bottom"
    	android:paddingBottom="5dp"
    	android:layout_marginLeft="10dp"
    	android:layout_marginRight="10dp"
    	android:layout_marginBottom="10dp"
    	android:layout_marginTop="10dp"
    	style="?android:attr/borderlessButtonStyle"
    	android:background="@drawable/mainpower"  
         />
        
         <Button 
        android:id="@+id/health_battery"
      	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_weight="2"
     	android:background="@drawable/batteryvoltage" 
     	android:layout_marginLeft="10dp"
    	android:layout_marginRight="10dp"
    	android:layout_marginBottom="10dp"
    	android:layout_marginTop="10dp"
    	android:text="" 
    	android:textColor="@android:color/white"
    	android:gravity="center_horizontal|bottom"
    	android:paddingBottom="5dp"
    	style="?android:attr/borderlessButtonStyle"
             />
        
        </LinearLayout>   
      <LinearLayout 
        android:layout_width="match_parent"
    	android:layout_height="wrap_content"
       	android:background="#25242a" 
    	android:orientation="horizontal"
    	android:layout_marginTop="10dp"
    	
            >
         <Button 
        android:id="@+id/health_temp"
      	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_weight="2"
    	android:paddingBottom="5dp"
    	android:layout_marginLeft="10dp"
    	android:layout_marginRight="10dp"
    	android:layout_marginBottom="10dp"
    	android:layout_marginTop="10dp"
    	android:background="@drawable/temperature"   
        style="?android:attr/borderlessButtonStyle"
        />
        
         <Button 
             android:id="@+id/health_light_intensity"
      	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_weight="2"
 		android:paddingBottom="5dp"
    	android:layout_marginLeft="10dp"
    	android:layout_marginRight="10dp"
    	android:layout_marginBottom="10dp"
    	android:layout_marginTop="10dp"
    	android:background="@drawable/light_intensity"
    	style="?android:attr/borderlessButtonStyle" 
             />
          <Button 
             android:id="@+id/health_qi_charge"
      	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_weight="2"
  		android:background="@drawable/q1_charging" 
    	 android:paddingBottom="5dp"
    	android:layout_marginLeft="10dp"
    	android:layout_marginRight="10dp"
    	android:layout_marginBottom="10dp"
    	android:layout_marginTop="10dp"
    	style="?android:attr/borderlessButtonStyle"
             />
       </LinearLayout>  
        <LinearLayout 
        android:layout_width="match_parent"
    	android:layout_height="wrap_content"
       	android:background="#25242a" 
    	android:orientation="horizontal"
    	android:layout_marginTop="10dp"
            >
         <Button 
        android:id="@+id/health_current"
      	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_weight="2"
    	 android:paddingBottom="5dp"
    	android:layout_marginLeft="10dp"
    	android:layout_marginRight="10dp"
    	android:layout_marginBottom="10dp"
    	android:layout_marginTop="10dp"
    	android:background="@drawable/q1_current"   
    	style="?android:attr/borderlessButtonStyle"
        />
        
         <Button 
             android:id="@+id/health_control"
      	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_weight="1"
  		android:paddingBottom="5dp"
    	android:layout_marginLeft="10dp"
    	android:layout_marginRight="10dp"
    	android:layout_marginBottom="10dp"
    	android:layout_marginTop="10dp"
    	android:background="@drawable/q1_control" 
    	style="?android:attr/borderlessButtonStyle"
             />
         
       </LinearLayout>      
    </LinearLayout>
    